@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<Manager.Host.PermitView.HaulerInfo>
@using Manager.Helpers
@{
    Layout = null;
}
<fieldset>
    <legend>Hauler Information</legend>
    @Html.GroupItem(Model.Name, "Name", "Name", true)
    @Html.GroupItem(Model.Date, "Date", "Application Date")
    @Html.GroupItem(Model.Email, "email", "Email", true)
    @Html.GroupItem(Model.Phone, "phone", "Phone", true)
    @Html.GroupItem(Model.Fax, "fax", "Fax")
</fieldset>
